{
  "name": "notepad",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "node ./dist/apps/api/main.js",
    "build": "nx run-many --target=build --projects=notepad,api --parallel=true",
    "test": "nx test",
    "lint": "nx workspace-lint && ng lint",
    "lint:styles": "nx run-many --target=lint-styles --all --fix --parallel",
    "lint:html": "htmllint \"apps/**/*.html\" \"libs/**/*.html\"",
    "lint-all": "npx nx run-many --all --target=lint"
  },
  "private": true,
